{
  "name": "chatmx",
  "version": "1.0.50",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/index.tsx\" \"bun run uno:dev\"",
    "uno": "bunx --bun unocss",
    "uno:dev": "bunx --bun unocss --watch",
    "typecheck": "bunx --bun tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint": "eslint . --report-unused-disable-directives",
    "lint:fix": "eslint . --report-unused-disable-directives --fix"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "^0.0.14",
    "@elysiajs/cron": "^0.8.0",
    "@elysiajs/html": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "@libsql/client": "^0.4.0-pre.5",
    "@t3-oss/env-core": "^0.7.1",
    "@tlscipher/holt": "^1.1.3",
    "drizzle-orm": "^0.29.3",
    "elysia": "latest",
    "pino-pretty": "^10.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^1.3.3",
    "@unocss/transformer-variant-group": "^0.58.2",
    "better-sqlite3": "^9.2.2",
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.9",
    "pino": "^8.17.2",
    "unocss": "^0.58.3"
  },
  "module": "src/index.js"
}
